date the path to your actual image location\nimport G10 from '../assets/P_CMACGM.webp'; // Update the path to your actual image location\nimport G11 from '../assets/P_Copart.webp'; // Update the path to your actual image location\n\nexport default {\n  name: 'Partners',\n  data() {\n    return {\n      images: [G1, G2, G3, G4, G5, G6, G7, G8, G9, G10, G11],\n      clonedImages: []\n    };\n  },\n  mounted() {\n    this.setupInfiniteScroll();\n    this.setInitialScroll();\n  },\n  methods: {\n    setupInfiniteScroll() {\n      // Clone the images array to simulate infinite scroll\n      this.clonedImages = [...this.images, ...this.images, ...this.images];\n    },\n    setInitialScroll() {\n      const container = this.$refs.scrollContainer;\n      const middlePosition = container.scrollWidth / 3; // Divide by 3 to get to the middle set of images\n      container.scrollLeft = middlePosition;\n    },\n    handleScroll() {\n      const container = this.$refs.scrollContainer;\n      const middlePosition = container.scrollWidth / 3;\n\n      if (container.scrollLeft <= 0) {\n        container.scrollLeft = middlePosition;\n      } else if (container.scrollLeft >= middlePosition * 2) {\n        container.scrollLeft = middlePosition;\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n.partners-section {\n  text-align: center;\n  padding: 20px;\n}\n.partners-title {\n  font-size: 36px;\n  margin-bottom: 10px;\n  font-weight: 500;\n}\n.partners-subtitle {\n  font-size: 34px;\n  margin-bottom: 20px;\n  font-weight: 500;\n}\n.scroll-container {\n  display: flex;\n  overflow-x: auto;\n  padding: 10px;\n  scrollbar-width: none; /* Firefox */\n}\n.scroll-container::-webkit-scrollbar {\n  display: none; /* Chrome, Safari and Opera */\n}\n.partner-logo {\n  flex: 0 0 auto;\n  margin: 0 10px;\n  max-width: 200px;\n  min-width: 100px;\n  padding: 10px;\n  background-color: white;\n  border-radius: 8px;\n  box-shadow: 0 2px 5px rgba(0, 0, 0, 0.1);\n}\n.partner-logo img {\n  display: block;\n  width: 100%;\n  height: auto;\n}\n</style>\n"],"mappings":"AAuBA,OAAOA,EAAC,MAAO,wBAAwB,EAAE;AACzC,OAAOC,EAAC,MAAO,qBAAqB,EAAE;AACtC,OAAOC,EAAC,MAAO,sBAAsB,EAAE;AACvC,OAAOC,EAAC,MAAO,yBAAyB,EAAE;AAC1C,OAAOC,EAAC,MAAO,sBAAsB,EAAE;AACvC,OAAOC,EAAC,MAAO,gCAAgC,EAAE;AACjD,OAAOC,EAAC,MAAO,2BAA2B,EAAE;AAC5C,OAAOC,EAAC,MAAO,oBAAoB,EAAE;AACrC,OAAOC,EAAC,MAAO,6BAA6B,EAAE;AAC9C,OAAOC,GAAE,MAAO,yBAAyB,EAAE;AAC3C,OAAOC,GAAE,MAAO,yBAAyB,EAAE;;AAE3C,eAAe;EACbC,IAAI,EAAE,UAAU;EAChBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,CAACb,EAAE,EAAEC,EAAE,EAAEC,EAAE,EAAEC,EAAE,EAAEC,EAAE,EAAEC,EAAE,EAAEC,EAAE,EAAEC,EAAE,EAAEC,EAAE,EAAEC,GAAG,EAAEC,GAAG,CAAC;MACtDI,YAAY,EAAE;IAChB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACC,mBAAmB,CAAC,CAAC;IAC1B,IAAI,CAACC,gBAAgB,CAAC,CAAC;EACzB,CAAC;EACDC,OAAO,EAAE;IACPF,mBAAmBA,CAAA,EAAG;MACpB;MACA,IAAI,CAACF,YAAW,GAAI,CAAC,GAAG,IAAI,CAACD,MAAM,EAAE,GAAG,IAAI,CAACA,MAAM,EAAE,GAAG,IAAI,CAACA,MAAM,CAAC;IACtE,CAAC;IACDI,gBAAgBA,CAAA,EAAG;MACjB,MAAME,SAAQ,GAAI,IAAI,CAACC,KAAK,CAACC,eAAe;MAC5C,MAAMC,cAAa,GAAIH,SAAS,CAACI,WAAU,GAAI,CAAC,EAAE;MAClDJ,SAAS,CAACK,UAAS,GAAIF,cAAc;IACvC,CAAC;IACDG,YAAYA,CAAA,EAAG;MACb,MAAMN,SAAQ,GAAI,IAAI,CAACC,KAAK,CAACC,eAAe;MAC5C,MAAMC,cAAa,GAAIH,SAAS,CAACI,WAAU,GAAI,CAAC;MAEhD,IAAIJ,SAAS,CAACK,UAAS,IAAK,CAAC,EAAE;QAC7BL,SAAS,CAACK,UAAS,GAAIF,cAAc;MACvC,OAAO,IAAIH,SAAS,CAACK,UAAS,IAAKF,cAAa,GAAI,CAAC,EAAE;QACrDH,SAAS,CAACK,UAAS,GAAIF,cAAc;MACvC;IACF;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}