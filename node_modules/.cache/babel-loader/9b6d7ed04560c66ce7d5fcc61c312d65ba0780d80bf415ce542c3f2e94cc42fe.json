ntIndex++;\n      }\n    },\n    updateCardWidth() {\n      if (this.$refs.carouselWrapper) {\n        this.cardWidth = this.$refs.carouselWrapper.clientWidth / 3; // Adjust for the number of cards visible\n      }\n    }\n  }\n};\n</script>\n\n<style scoped>\n.carousel-container {\n  position: relative;\n  overflow: hidden;\n  width: 100%;\n  max-width: 1000px;\n  margin: auto;\n}\n\n.carousel-wrapper {\n  overflow: hidden;\n}\n\n.carousel {\n  display: flex;\n  transition: transform 0.3s ease-in-out;\n}\n\n.carousel-card {\n  flex: 0 0 33.33%; /* Adjust for the number of cards visible */\n  box-sizing: border-box;\n  height: 50px;\n  padding: 20px;\n}\n\n.card-content {\n  background-color: #a5a5e0;\n  border-radius: 10px;\n  text-align: center;\n}\n\n.gallery-image {\n  max-width: 100%;\n  border-radius: 10px;\n}\n\n.nav-buttons {\n  display: flex;\n  justify-content: center;\n  margin-top: 10px;\n}\n\n.nav-button {\n  background: #f04e45;\n  border: none;\n  color: white;\n  padding-inline: 10px;\n  font-size: 1.5rem;\n  margin: 0 5px;\n  cursor: pointer;\n}\n</style>\n"],"mappings":"AAmBA,SAASA,QAAO,QAAS,KAAK;AAE9B,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,CACL;QAAEC,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAEC,OAAO,CAAC,oBAAoB;MAAE,CAAC,EACzD;QAAEF,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAEC,OAAO,CAAC,oBAAoB;MAAE,CAAC,EACzD;QAAEF,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAEC,OAAO,CAAC,oBAAoB;MAAE,CAAC,EACzD;QAAEF,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAEC,OAAO,CAAC,oBAAoB;MAAE,CAAC,EACzD;QAAEF,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAEC,OAAO,CAAC,oBAAoB;MAAE,CAAC,EACzD;QAAEF,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAEC,OAAO,CAAC,oBAAoB;MAAE,CAAC,EACzD;QAAEF,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAEC,OAAO,CAAC,oBAAoB;MAAE,CAAC,EACzD;QAAEF,KAAK,EAAE,QAAQ;QAAEC,KAAK,EAAEC,OAAO,CAAC,oBAAoB;MAAE,CAAC,CAC1D;MACDC,YAAY,EAAE,CAAC;MACfC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACRR,QAAQ,CAAC,IAAI,CAACS,eAAe,CAAC,EAAE;IAChCC,MAAM,CAACC,gBAAgB,CAAC,QAAQ,EAAE,IAAI,CAACF,eAAe,CAAC;EACzD,CAAC;EACDG,aAAaA,CAAA,EAAG;IACdF,MAAM,CAACG,mBAA